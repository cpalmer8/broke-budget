<%= will_paginate @goals %>
    <%= @goals.each do |goal| %>
      <p><%= goal.name %></p>
      <p><%= link_to 'Show', goal %><p>
      <p><%= link_to 'Edit',  edit_goal_path(goal), remote:true %></p>
      <p><%= link_to 'Destroy', goal, method: :delete, data: { confirm: 'Are you sure?' } %></p>
    <% end %>

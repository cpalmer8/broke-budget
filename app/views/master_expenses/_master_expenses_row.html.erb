<%= will_paginate @expenses %>
<% expenses.each do |expense| %>
  <p><%= expense.amount %></p>
  <p><%= link_to 'Show', expense %><p>
  <p><a data-toggle="modal" data-target="#editModal" data-remote="<%= edit_master_expense_path(expense) %> #modal-edit-fields" class="btn btn-mini">Edit</a>
  <p><%= link_to 'Destroy', expense, method: :delete, data: { confirm: 'Are you sure?' } %></p>
<% end %>

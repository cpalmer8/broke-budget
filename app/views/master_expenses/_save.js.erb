$("ul.errors").html("")
<% if @master_expense.errors.any? %>
  <% @master_expense.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".master-expense-index").html("<%= escape_javascript(render 'index') %>")
  $("#edit-modal").modal("hide")
<% end %>
